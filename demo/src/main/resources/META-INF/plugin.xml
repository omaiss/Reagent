<idea-plugin>
    <id>fyp</id>
    <name>Reagent</name>
    <version>1.0</version>
    <vendor email="support@example.com" url="https://example.com">Example</vendor>
    <description>This is a sample PyCharm plugin that I will be making.</description>
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <annotator language="" implementationClass="fyp.PEP8Annotator"/>
        <editorFactoryDocumentListener implementation="fyp.AITalker"/>
    </extensions>
</idea-plugin>
