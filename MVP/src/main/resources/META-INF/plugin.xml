<idea-plugin>
    <id>com.example.mvp</id>
    <name>MVP</name>
    <version>1.0.0</version>
    <vendor email="you@example.com" url="http://yourwebsite.com">Your Name</vendor>
    <description>The plugin will detect code vunerabilities and help format the code using PEP8 coding standards
    </description>

    <!-- Register actions, extensions, etc. -->
    <actions>
        <action id="com.example.mvp.DisplayPopupAction" class="com.example.mvp.DisplayPopupAction" text="Show Popup"
                description="Show Popup Message">
            <add-to-group group-id="EditorPopupMenu" anchor="last"/>
        </action>
    </actions>
</idea-plugin>
